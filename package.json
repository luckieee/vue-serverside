{
    "name": "vue-ssr-telefonica",
    "version": "1.0.0",
    "description": "Telefonica app vue server side micro fed",
    "scripts": {
        "dev": "npm run build && node server.js",
        "build": "npm run build:client && npm run build:server",
        "build:client": "webpack --config build/webpack.client.config.js --progress --hide-modules",
        "build:server": "webpack --config build/webpack.server.config.js --progress --hide-modules",
        "compile": "webpack --config webpack.config.js"
    },
    "dependencies": {
        "cross-env": "^7.0.3",
        "es6-promise": "^4.2.8",
        "express": "^4.17.1",
        "fs": "*",
        "vue": "^2.6.12",
        "vue-router": "^3.5.1",
        "vue-server-renderer": "^2.6.12",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "autoprefixer": "^10.2.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.2.2",
        "chokidar": "^3.5.1",
        "css-loader": "^5.2.4",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^6.2.0",
        "memory-fs": "^0.5.0",
        "rimraf": "^3.0.2",
        "sass": "^1.32.11",
        "sass-loader": "^10.1.1",
        "url-loader": "^4.1.1",
        "vue-loader": "^15.9.6",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.35.1",
        "webpack-cli": "^4.6.0",
        "webpack-dev-middleware": "^4.1.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^5.7.3",
        "webpack-node-externals": "^3.0.0"
    }
}
